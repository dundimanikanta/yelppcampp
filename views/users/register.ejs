                     <input class="form-control" type="password" id="pass" name="password" required>
                 </div>

                 <div class="mb-3 d-flex justify-content-center">         
                    <button  class="btn btn-success">submit</button>
                 </div>
                 
             </form> 
         </div>
     </div>
 
